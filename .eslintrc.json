{
    "root": true,
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "react/display-name": 2,
      "react/jsx-filename-extension":0,
      "semi": [ 2, "never" ],
      "max-len": [2, 100],
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "operator-linebreak": 0
    }
}

